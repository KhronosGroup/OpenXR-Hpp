{
  "tab_size": 4,
  "dangle_parens": true,
  "dangle_align": "prefix",
  "line_ending": "unix",
  "command_case": "canonical",
  "keyword_case": "upper",
  "additional_commands": {
    "ezvcpkg_fetch": {
      "flags": [
        "UPDATE_TOOLCHAIN",
        "USE_HOST_VCPKG"
      ],
      "kwargs": {
        "PACKAGES": "+",
        "OUTPUT": 1
      }
    }
  },
  "always_wrap": [
    "set_source_files_properties",
    "install"
  ],
  "first_comment_is_literal": true
}
